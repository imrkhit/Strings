#include<stdio.h>
#include<conio.h>
#include<stdlib.h>
#include<string.h>
int main()
{
    char str[1000];
    printf("Enter the string\n");
    gets(str);
    firstunique(str);
    return;
}

void firstunique(char *str)
{
    int i=0, j, temp;
    while(str[i])
    {
        temp=str[i];
        j=i+1;
        while(str[j])
        {
            if(temp!=str[j])
            {
                j++;
                continue;
            }
            else
                break;

        }
        if(j==strlen(str))
        {
            printf("%c", str[i]);
            return;
        }
        else
        {
            i++;
        }
    }
}
