#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#include<conio.h>
#define no_of_characters 256
void fillcharcounts(char *str, int *count);
void printdups(char *str);
int main()
{
    char str[]="hbaidewknwekwoe";
    printdups(str);
    getchar();
    return;

}

void printdups(char *str)
{
    int *count=(int*)calloc(no_of_characters,sizeof(int*));
    fillcharcounts(str,count);
    int i;
    for(i=0;i<no_of_characters;i++)
    {
        if(count[i]>1)
            printf("%c, count=%d\n", i, count[i]);
    }
    free(count);
    return;
}

void fillcharcounts(char *str, int *count)
{
    int i;
    for(i=0;*(str+i);i++)
    {
        count[*(str+i)]++;
    }
    return;
}
