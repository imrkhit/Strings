#include<stdio.h>
#include<conio.h>
#include<string.h>
int main()
{
    char str[]="Hi I am Rohit";
    int i, len=strlen(str);
    for(i=len-1;i>=0;i--)
    {
        if(str[i]==' ')
        {
            str[i]='\0';
            printf("%s ", &(str[i])+1);
        }
    }
    printf("%s", str);
    return;
}
